{
  "name": "mesaifinal-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "echo 'Skipping husky install'",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "analyze:dev": "npx vite-bundle-analyzer --mode development",
    "lighthouse": "npx lighthouse http://localhost:5173 --output=html --output-path=./lighthouse-report.html",
    "lighthouse:ci": "npx lighthouse ci",
    "performance": "npm run build && npm run lighthouse",
    "bundle-size": "npm run build && npx bundle-size",
    "optimize": "npm run build && npm run analyze && npm run lighthouse"
  },
  "dependencies": {
    "@progress/kendo-data-query": "^1.0.0",
    "@progress/kendo-drawing": "^1.0.0",
    "@progress/kendo-licensing": "^1.7.1",
    "@progress/kendo-react-all": "^6.0.0",
    "@progress/kendo-react-buttons": "^6.1.1",
    "@progress/kendo-react-charts": "^6.0.0",
    "@progress/kendo-react-common": "^6.0.0",
    "@progress/kendo-react-data-tools": "^6.0.0",
    "@progress/kendo-react-dropdowns": "^6.1.1",
    "@progress/kendo-react-form": "^6.1.1",
    "@progress/kendo-react-grid": "^6.0.0",
    "@progress/kendo-react-indicators": "^6.0.0",
    "@progress/kendo-react-inputs": "^6.0.0",
    "@progress/kendo-react-intl": "^6.0.0",
    "@progress/kendo-react-layout": "^6.0.0",
    "@progress/kendo-react-popup": "^6.0.0",
    "@progress/kendo-react-upload": "^6.0.0",
    "@progress/kendo-svg-icons": "^2.3.0",
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-query-devtools": "^5.17.0",
    "axios": "^1.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.0",
    "react-toastify": "^11.0.5",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.0.1",
    "bundle-size": "^0.3.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lighthouse": "^12.8.1",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
